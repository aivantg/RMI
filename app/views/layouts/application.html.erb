<!DOCTYPE html>
<html>
  <head>
    <title>Rmi</title>
    <%= csrf_meta_tags %>

    <script>
      window.INITIAL_STATE = <%= raw @state.to_json %>
    </script>
  </head>

  <body>
    <%= javascript_pack_tag 'application' %>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
